//Crie um algoritmo que transforme grau em radiano, conforme as funções seno, cosseno e tangente. a)30 b)90 c)180 d)270
programa {
  funcao grauToRad(inteiro grau){
    escreva(""+grau+" graus  == "+(grau/180)+" pi rad\n")
  }
  funcao inicio() {
    grauToRad(30)
    grauToRad(90)
    grauToRad(180)
    grauToRad(270)    

  }
}
